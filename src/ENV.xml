            <developer>true</developer> <!-- It is important to the method of debug, true is full debug -->
            <emailLog>test@gmail.com,example2@gmail.com</emailLog> <!-- List of comma e-mails, no space -->
            <siteName>Moje MVC</siteName> <!-- Site name -->
        </siteConfig>
        <dbConfig>
            <dbHost>localhost</dbHost>
            <dbName>dbname</dbName>
